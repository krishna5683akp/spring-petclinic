pipeline {
    agent {label 'JAVA11'}
    stages {
        stage ('vcs') {
            steps {
                        git branch: 'NEW_PROJECT_1', url: 'https://github.com/krishna5683akp/spring-petclinic.git'

            }
        }
        stage('build') {
            steps {
                sh 'mvn package'
            }
        }
        stage('archive junit') {
            steps {
                junit '**/surefire-reports/*xml'
            }
        }
        stage('archive artifacts') {
            steps {
                archive 'target/*.jar'
            }
        }
    }
}